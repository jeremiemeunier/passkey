{
  "name": "@jeremiemeunier/passkey",
  "version": "0.1.3",
  "description": "Client-side passkey authentication package with React hook",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "pub": "tsup && npm publish"
  },
  "keywords": [
    "passkey",
    "webauthn",
    "authentication",
    "react",
    "hook"
  ],
  "publishConfig": {
    "@jeremiemeunier:registry": "https://npm.pkg.github.com"
  },
  "author": "Jeremie Meunier",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "react": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "@simplewebauthn/browser": "^9.0.0",
    "axios": "^1.13.2"
  }
}
